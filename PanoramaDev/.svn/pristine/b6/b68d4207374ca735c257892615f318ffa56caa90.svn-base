package panorama;

import java.io.Serializable;

/**
 *
 * @author clocal
 */
public class CoursTempo implements Serializable
{
    // <editor-fold defaultstate="collapsed" desc="Variables">
    private String departement_;
    private String titre_;
    private String numero_;
    //</editor-fold>
    
    // <editor-fold defaultstate="collapsed" desc="Constructeurs">
    public CoursTempo()
    {
        
    }
    
    public CoursTempo(String departement, String titre, String numero)
    {
        departement_ = departement;
        titre_ = titre;
        numero_ = numero;
    }
    //</editor-fold>
    
    // <editor-fold defaultstate="collapsed" desc="getter / setter">
    public String getDepartement_() {
        return departement_;
    }

    public void setDepartement_(String departement_) {
        this.departement_ = departement_;
    }

    public String getTitre_() {
        return titre_;
    }

    public void setTitre_(String titre_) {
        this.titre_ = titre_;
    }

    public String getNumero_() {
        return numero_;
    }

    public void setNumero_(String numero_) {
        this.numero_ = numero_;
    }
    // </editor-fold>
}
