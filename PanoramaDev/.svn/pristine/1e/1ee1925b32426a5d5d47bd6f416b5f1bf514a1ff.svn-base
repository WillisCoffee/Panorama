    public void setUnEtudiant(Etudiant unEtudiant) {
        Etudiant initial = _etudiant;
        _etudiant = unEtudiant;
        propertyChangeSupport.firePropertyChange("etudiant", initial, 
                                                 _etudiant);
    }
    
